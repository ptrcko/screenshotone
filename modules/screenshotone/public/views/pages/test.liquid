{% liquid
    comment 
        update these variables to point to a database record and url to test 
    endcomment
    assign id = 1234 
    assign table = "modules/example/table"
    assign name = "example_name"
    assign url = "https://www.example.com"
    assign image = null


%}

{% liquid
    function result = "modules/screenshotone/shoot",
      table: table,
      id: id,
      url: url,
      name: name,
      image: image
    assign url = result | dig: "record_update", "image", "url"
%}

<img src="{{ url }}" alt="Screenshot">
